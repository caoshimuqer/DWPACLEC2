#original for 8055.dic
SET sql_mode = 'STRICT_TRANS_TABLES';
SET sql_mode = 'STRICT_ALL_TABLES';
CREATE TABLE DICT1 (WORD CHAR(20) NOT NULL UNIQUE,LENGTH TINYINT UNSIGNED NOT NULL,CHECK (LENGTH>7), PRIMARY KEY (WORD)) CHARACTER SET ascii;
 LOAD DATA LOCAL INFILE '/home/calpoly/DWPACLEC2/src/slave/test/8055.dic' IGNORE INTO TABLE DICT1 CHARACTER SET ascii LINES TERMINATED BY '\n' (@word) SET WORD=@word,LENGTH = CHAR_LENGTH(word);
ALTER TABLE `DICT1` ADD INDEX `WORD` (`WORD`);

